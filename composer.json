{
    "name": "spomky-labs/jose",
    "description": "JSON Object Signing and Encryption library for PHP and Symfony Bundle.",
    "type": "library",
    "license": "MIT",
    "keywords": ["JWS", "JWT", "JWE", "JWA", "JWK", "JWKSet", "Jot", "Jose", "RFC7515", "RFC7516", "RFC7517", "RFC7518", "RFC7519", "RFC7520", "Bundle", "Symfony"],
    "homepage": "https://github.com/Spomky-Labs/Jose",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },{
            "name": "All contributors",
            "homepage": "https://github.com/Spomky-Labs/jose/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jose\\":  "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jose\\Test\\":  "tests/",
            "Jose\\Performance\\":  "performance/"
        }
    },
    "require": {
        "php": "^7.1",
        "lib-openssl": "*",
        "ext-mbstring": "*",
        "ext-gmp": "*",
        "spomky-labs/base64url": "^1.0",
        "spomky-labs/aes-key-wrap": "^4.0",
        "fgrosse/phpasn1": "^1.5",
        "php-http/httplug": "^1.1",
        "php-http/message-factory": "^1.0",
        "paragonie/sodium_compat": "^1.1",
        "symfony/console": "^3.3",
        "padraic/phar-updater": "^1.0",
        "symfony/http-kernel": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3"
    },
    "require-dev": {
        "ext-curl": "*",
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "bjeavons/zxcvbn-php": "^0.3.0",
        "mdanter/ecc": "0.4.*",
        "phpbench/phpbench": "^0.13.0",
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^1.0",
        "symfony/finder": "^3.3"
    },
   "suggest":{
        "mdanter/ecc": "For ECDH-ES with P-256, P-384 and P-521 Elliptic Curves algorithms support.",
        "bjeavons/zxcvbn-php": "Adds key quality check for oct keys."
   },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
